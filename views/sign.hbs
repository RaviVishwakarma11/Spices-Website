        }

        .navbar-brand img {
            width: 70px;
            border: 1px solid white;
            border-radius: 80px;
        }

        section{
            margin: 30px 0px;
        }

        .collapse .navbar-nav .nav-item a {
            color: black;

        }

        .collapse .d-flex .form-control {
            border-radius: 10px;
            background-color: white;
        }

        .collapse .d-flex .btn {
            background-color: #DC143C;
            color: white;
            border: 2px solid white;
            border-radius: 5px;
        }

        .container-fluid .row .col-md-4 .card {
            display: flex;
        }

        .container-fluid .row .col-md-4 .card h5 {
            color: black;
            margin-left: 50px;
        }

        #selection-option {
         background-color: rgb(212, 88, 113);
